{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "paths": {
        "@types/*": ["types/*"]
      },  
      "baseUrl": "./",
    "skipLibCheck": true,
    "typeRoots": [
      "./types"
    ],
    "strict": true
  },
  "exclude":[
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "include": [
    "data/index.js"
      ],
  "extends": "expo/tsconfig.base"
}
